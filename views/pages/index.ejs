    var data = {}, marker, map;

    function initMap(obj) {
        map = new google.maps.Map(document.getElementById('map'), {
            center: new google.maps.LatLng(obj.lat, obj.lng),
            scrollwheel: false,
            zoom: 8
        });

        marker = new google.maps.Marker({
            map: map,
            position: obj ,
            title: 'Find out Locations of University!!!'
        });
    }

    function setMarkerPosition (lat, long) {
        var loc = new google.maps.LatLng(lat, long);
        marker.setPosition(loc);
        map.setCenter(marker.getPosition());
    }

    $(document).ready(function () {



        $('#GetData').click(function () {

            $.getJSON('https://finalcse53351001121600.herokuapp.com/getUniversityList', function (data) {

                data = data; // store gloally
                alert(data[0]);
                /* By default first object in array will be center */
                var loc = {
                    lat: data.universitylist[0].lat,
                    lng: data.universitylist[0].lng
                };

                /* Init google map and initializae marker */
                initMap(loc);

                var output = "<table border='1'>"+   "<tr>" +
                    "<th>"+ 'University Name'+ "</th>"+
                    "<th>" + 'Location' +"</th>"+
                    "<th>" + 'State' +"</th>"
                "</tr>"


                for (var i in data.universitylist)
                    output += "<tr>" + "<td onclick=\"setMarkerPosition('"+ data.universitylist[i].lat +"', '"+ data.universitylist[i].lng +"')\">"
                        + data.universitylist[i].Name +
                        "</td>" +
                        "<td>" +
                        data.universitylist[i].City + "</td>" +
                        "</td>" +
                        "<td>" +
                        data.universitylist[i].State +
                        "</td>" +
                        "</tr>";
                output += "</table>";

                console.log(output);

                $("#show-data").html(output);
            });

